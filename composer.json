{
  "name": "birkof/netopia-mobilpay-bundle",
  "description": "Seamless integration of Netopia MobilPay Payment Gateway into your Symfony application",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "birkof\\NetopiaMobilPay\\": "src/"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "birkof/netopia-mobilpay": "^4.0",
    "symfony/monolog-bundle": "^3.7",
    "symfony/routing": "^4.4 || ^5.0 || ^6.0"
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Bobnet-Tech/netopia-mobilpay"
    }
  ]
}
